<a href="/post">add details</a>
    <table border="1">
        <tr>
            <td>title</td>
            <td>content</td>
            <td>image</td>
            <td>Action</td>
        </tr>
        <% for(var pd of alldata) { %>
            <tr>
                <td><%= pd.title %></td>
                <td><%= pd.content %></td>
                <td><img src="<%= pd.postImage %>" height="100"></td>
                <td><a href="/post/deletepost/<%= pd.id %>">Delete</a></td>
            </tr>
        <% } %>
     </table>